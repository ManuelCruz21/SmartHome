@startuml
'https://plantuml.com/sequence-diagram
!pragma layout smetana

autonumber

actor "User" as User
participant "RestClient" as RC

box "SmartHome"
    participant "Server" as SV
    participant "DataBase" as BD
end box

User -> RC : I want to get the list of rooms
activate RC

RC -> SV : getAllRooms()
activate SV

SV -> BD : findAll()
activate BD

BD -> SV : roomDataModelList
deactivate BD

SV --> RC : roomDTOList
deactivate SV

RC --> User : roomDTOList
deactivate RC


@enduml